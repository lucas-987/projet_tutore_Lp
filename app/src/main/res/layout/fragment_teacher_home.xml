<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".TeacherHomeFragment">

    <Button
        android:id="@+id/teacherHome_newCall_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/teacherHome_newCall"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.44" />

    <Button
        android:id="@+id/teacherHome_updateLastCall_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/teacherHome_updateLastCall"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/teacherHome_newCall_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.15"/>


</androidx.constraintlayout.widget.ConstraintLayout>